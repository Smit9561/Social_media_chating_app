<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity14">

    <RelativeLayout
        android:id="@id/cc1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        android:orientation="horizontal">


        <RelativeLayout
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentLeft="true"
            android:background="@drawable/button_round_dark_white"
            android:orientation="horizontal">


            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:background="@drawable/button_round_dark_white"
                android:paddingHorizontal="5dp"
                android:paddingVertical="5dp"
                android:src="@drawable/settings" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true">


            <RelativeLayout

                android:layout_width="150dp"
                android:layout_height="50dp"
                android:layout_alignParentRight="true"
                android:background="@drawable/button_round_dark_white">


                <TextView
                    android:id="@+id/i2"
                    android:layout_width="120dp"
                    android:layout_height="25dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="30dp"
                    android:text="Match"
                    android:textAlignment="center"
                    android:textColor="@color/black"
                    android:textSize="20dp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="120dp"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_centerInParent="true"
                android:background="@drawable/button_round_black">


                <TextView
                    android:layout_width="120dp"
                    android:layout_height="25dp"
                    android:layout_centerInParent="true"
                    android:text="Discovery"
                    android:textAlignment="center"
                    android:textColor="@color/white"
                    android:textSize="20dp" />
            </RelativeLayout>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:background="@drawable/button_round_dark_white">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:background="@drawable/button_round_dark_white"
                android:paddingHorizontal="7dp"
                android:paddingVertical="7dp"
                android:src="@drawable/bell" />
        </RelativeLayout>

    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/lastView"
        android:layout_below="@+id/cc1">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="10dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:id="@+id/jj1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/layout_14_message_girls"
                        android:layout_weight="1"
                        android:layout_margin="5dp">

                        <ImageView
                            android:id="@+id/pp1"
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:layout_alignParentBottom="true"
                            android:layout_marginLeft="15dp"
                            android:layout_marginBottom="10dp"
                            android:src="@drawable/green_dot" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Hailey, 21"
                            android:layout_toRightOf="@id/pp1"
                            android:textColor="@color/white"
                           android:layout_above="@+id/bb1"
                            android:textSize="20dp"/>

                        <TextView
                            android:id="@+id/bb1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_toRightOf="@id/pp1"
                            android:text="Active"
                            android:layout_margin="5dp"
                            android:textColor="@color/white"
                            android:textSize="15dp" />
                    </RelativeLayout>


                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/layout_14_message_girls_2"
                        android:layout_weight="1"
                        android:layout_margin="5dp">

                        <ImageView
                            android:id="@+id/ppp1"
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:layout_alignParentBottom="true"
                            android:layout_marginLeft="15dp"
                            android:layout_marginBottom="10dp"
                            android:src="@drawable/green_dot" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Hailey, 21"
                            android:layout_toRightOf="@id/ppp1"
                            android:textColor="@color/white"
                            android:layout_above="@+id/bbb1"
                            android:textSize="20dp"/>

                        <TextView
                            android:id="@+id/bbb1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_toRightOf="@id/ppp1"
                            android:text="Active"
                            android:layout_margin="5dp"
                            android:textColor="@color/white"
                            android:textSize="15dp" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/layout_14_message_girls_3"
                        android:layout_weight="1"
                        android:layout_margin="5dp">

                        <ImageView
                            android:id="@+id/pppp1"
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:layout_alignParentBottom="true"
                            android:layout_marginLeft="15dp"
                            android:layout_marginBottom="10dp"
                            android:src="@drawable/green_dot" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Hailey, 21"
                            android:layout_toRightOf="@id/pppp1"
                            android:textColor="@color/white"
                            android:layout_above="@+id/bbbb1"
                            android:textSize="20dp"/>

                        <TextView
                            android:id="@+id/bbbb1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_toRightOf="@id/pppp1"
                            android:text="Active"
                            android:layout_margin="5dp"
                            android:textColor="@color/white"
                            android:textSize="15dp" />
                    </RelativeLayout>


                    <RelativeLayout
                        android:id="@+id/ppp4"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/layout_14_message_girls_4"
                        android:layout_weight="1"
                        android:layout_margin="5dp">

                        <ImageView
                            android:id="@+id/ppppp1"
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:layout_alignParentBottom="true"
                            android:layout_marginLeft="15dp"
                            android:layout_marginBottom="10dp"
                            android:src="@drawable/green_dot" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Hailey, 21"
                            android:layout_toRightOf="@id/ppppp1"
                            android:textColor="@color/white"
                            android:layout_above="@+id/bbbbb1"
                            android:textSize="20dp"/>

                        <TextView
                            android:id="@+id/bbbbb1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_toRightOf="@id/ppppp1"
                            android:text="Active"
                            android:layout_margin="5dp"
                            android:textColor="@color/white"
                            android:textSize="15dp" />
                    </RelativeLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/pp6"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@+id/pp3"
                        android:layout_margin="5dp"
                        android:layout_weight="1"
                        android:background="@drawable/cut_last_2_layout_14" />

                    <ImageView
                        android:id="@+id/pp5"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@id/ppp4"
                        android:layout_margin="5dp"
                        android:layout_toRightOf="@id/pp1"
                        android:layout_weight="1"
                        android:background="@drawable/cut_layout_14" />
                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>

    </ScrollView>

    <LinearLayout
        android:id="@+id/lastView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@color/white"
        android:orientation="horizontal"
        android:padding="15dp">


        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:clickable="true"
            android:src="@drawable/home" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:src="@drawable/heart" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:src="@drawable/chat" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:src="@drawable/admin" />

    </LinearLayout>

</RelativeLayout>