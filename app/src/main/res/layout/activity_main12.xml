<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity12">


    <RelativeLayout
        android:id="@+id/hh1"
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:background="@drawable/ss1">

        <ImageView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/left" />


    </RelativeLayout>

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/hh3"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_above="@+id/rr2"
        android:src="@drawable/button_round_semi" />

    <TextView
        android:id="@+id/rr2"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:layout_above="@id/hh2"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="-25dp"
        android:background="@drawable/background_layout_2"
        android:gravity="center"
        android:text="Suncity"
        android:textColor="@color/black"
        android:textSize="40dp"
        android:textStyle="bold" />


    <RelativeLayout
        android:id="@+id/hh2"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_below="@+id/hh1"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="30dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp">


        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/c1"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="5dp"
            android:src="@drawable/aaa1"
            app:civ_border_color="@color/white"
            app:civ_border_width="2dp" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="35dp"
            android:src="@drawable/g10"
            app:civ_border_color="@color/white"
            app:civ_border_width="2dp" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="60dp"
            android:src="@drawable/g4"
            app:civ_border_color="@color/white"
            app:civ_border_width="2dp" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="85dp"
            android:src="@drawable/aaa1"
            app:civ_border_color="@color/white"
            app:civ_border_width="2dp" />


        <TextView
            android:id="@+id/kk1"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="110dp"
            android:background="@drawable/button_round_black"
            android:gravity="center_vertical"
            android:padding="5dp"
            android:text="+23"
            android:textColor="@color/white"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/kk2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="10dp"
            android:gravity="center_vertical|center|center_horizontal"
            android:text="View all"
            android:textColor="#FF3A0F"
            android:textStyle="bold" />

        <LinearLayout
            android:id="@+id/l2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/c1">

            <RelativeLayout
                android:id="@+id/cc1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_margin="10dp"
                android:layout_weight="1"
                android:background="@drawable/pink_background_buttoun"
                android:paddingLeft="5dp"
                android:paddingTop="5dp"
                android:paddingBottom="5dp">


                <ImageView
                    android:id="@+id/hh4"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="5dp"
                    android:background="@drawable/button_round_pink2"
                    android:padding="10dp"
                    android:src="@drawable/price_tag" />

                <TextView
                    android:id="@+id/hh5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@id/hh4"
                    android:text="Price" />

                <TextView
                    android:id="@+id/hh6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/hh5"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@id/hh4"
                    android:lines="1"
                    android:text="$50.00"
                    android:textSize="15dp"
                    android:textStyle="bold" />


            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/cc2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_margin="10dp"
                android:layout_weight="1"
                android:background="@drawable/lite_yellow_background_buttoun"
                android:paddingLeft="5dp"
                android:paddingTop="5dp"
                android:paddingBottom="5dp">


                <ImageView
                    android:id="@+id/hh8"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="5dp"
                    android:background="@drawable/button_round_pink2"
                    android:padding="10dp"
                    android:src="@drawable/price_tag" />

                <TextView
                    android:id="@+id/hh9"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@id/hh8"
                    android:text="Price" />

                <TextView
                    android:id="@+id/hh10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/hh9"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@id/hh8"
                    android:lines="1"
                    android:text="$50.00"
                    android:textSize="15dp"
                    android:textStyle="bold" />


            </RelativeLayout>


        </LinearLayout>

        <TextView
            android:id="@+id/tt1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/l2"
            android:layout_marginLeft="5dp"
            android:text="About"
            android:textColor="@color/black"
            android:textSize="15dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tt2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tt1"
            android:layout_marginLeft="5dp"
            android:lines="3"
            android:text="It will be a great party of people from neples,italy! We will celebrate Friday with a new friend at the Suncity club. fell free to connect:)"
            android:textSize="18dp" />


        <LinearLayout
            android:id="@+id/ss9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/tt2"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/im1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/layout_6_girls" />


            <ImageView
                android:id="@+id/ii1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/layout_6_girls2" />


        </LinearLayout>


        <RelativeLayout
            android:id="@+id/gg1"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_below="@id/ss9"
            android:layout_marginTop="15dp"
            android:background="@drawable/dark_pink"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:paddingRight="25dp"
            android:paddingBottom="5dp">


            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/mm1"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_centerVertical="true"
                android:src="@drawable/right"
                app:civ_border_color="@color/white"
                app:civ_border_width="5dp" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center">

                <TextView
                    android:id="@+id/pi1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:text="Swipe and "
                    android:textColor="#D254FE" />

                <TextView
                    android:id="@+id/ff2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/pi1"
                    android:text=" let's go"
                    android:textColor="@color/white" />

            </RelativeLayout>
        </RelativeLayout>


    </RelativeLayout>

</RelativeLayout>